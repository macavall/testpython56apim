{
  "version": "2.0",
  "telemetryMode": "OpenTelemetry",
  "logging": {
    "fileLoggingMode": "always",
    "console": {
      "isEnabled": "true"
    },
    "logLevel": {
      "default": "Trace",
      "Host.Results": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace",
      "Microsoft": "Trace",
      "Host.Aggregator": "Trace",
      "Function": "Trace",
      "Host.Executor": "Trace"
    },
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}
